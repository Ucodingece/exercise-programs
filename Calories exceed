def main():
  meal1=450
  meal2=720
  meal3=610
  calories = getTotalCalories(meal1,meal2,meal3)
  if isWithinLimits(calories):
    print("Good")
  else:
    print("Bad")
def getTotalCalories(meal1,meal2,meal3):
  return meal1 + meal2 + meal3
def isWithinLimits(calories):
  return calories <= 1800
main()  
